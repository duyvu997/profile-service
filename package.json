{
  "name": "house-service",
  "version": "1.0.0",
  "description": "rent-house app",
  "main": "handler.js",
  "scripts": {
    "test": "mocha --timeout 10000 --exit -r ts-node/register './**/*.spec.ts' ",
    "test-with-coverage-dev": "nyc mocha --timeout 3000 --exit -r ts-node/register './**/**.spec.ts'",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage|.build|.nyc_output|.serverless)/**/}*.{js,jsx,ts,tsx,json}\"",
    "pre-commit": "yarn tslint:fix && yarn prettier && yarn test-with-coverage-dev",
    "tslint": "tslint --project tsconfig.json",
    "tslint:fix": "tslint --project tsconfig.json --fix",
    "debug": "export SLS_DEBUG=* && node --inspect ./node_modules/serverless/bin/serverless offline -s local --port 3000 --aws-profile highjump"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pre-commit"
    }
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "check-coverage": true,
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
  },
  "author": "vnd",
  "license": "MIT",
  "devDependencies": {
    "@types/aws-lambda": "8.10.40",
    "@types/bcrypt": "3.0.0",
    "@types/chai": "^4.1.7",
    "@types/jsonwebtoken": "8.3.7",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "5.7.0",
    "@types/node": "13.5.2",
    "@types/sinon": "^7.0.13",
    "@types/triple-beam": "1.3.0",
    "chai": "^4.2.0",
    "husky": "^3.0.2",
    "mocha": "^6.2.0",
    "mongo-unit": "^1.4.4",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "serverless": "1.62.0",
    "serverless-offline": "5.12.1",
    "serverless-plugin-typescript": "1.1.9",
    "sinon": "^7.4.1",
    "ts-node": "^8.3.0",
    "tslint": "6.0.0",
    "typescript": "3.7.5"
  },
  "dependencies": {
    "@hasezoey/typegoose": "5.9.2",
    "aws-lambda": "1.0.5",
    "aws-lambda-response-builder": "1.0.11",
    "bcrypt": "3.0.7",
    "class-transformer": "^0.2.3",
    "class-transformer-validator": "^0.8.0",
    "class-validator": "0.10.1",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.8.10",
    "triple-beam": "1.3.0",
    "winston": "3.2.1"
  }
}
